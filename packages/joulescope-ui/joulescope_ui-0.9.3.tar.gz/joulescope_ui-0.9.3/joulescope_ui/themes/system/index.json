{
  "type": "theme",
  "version": 1,
  "name": "system",
  "description": "System theme",
  "files": ["style.html", "style.qss"],
  "images": {
    "copy": {
      "sources": [
        "play.svg",
        "record.svg",
        "record_statistics.svg",
        "zoom_all.svg",
        "zoom_in.svg",
        "zoom_out.svg"
      ],
